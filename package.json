{
  "name": "collection-tracker",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/electron.js",
  "scripts": {
    "electron:compile": "tsc --skipLibCheck --p ./tsconfig.electron.json && electron .",
    "electron:start": "cross-env NODE_ENV=dev nodemon",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && tsc --skipLibCheck --p ./tsconfig.electron.json",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "author": "Alexander Nedomansky <anedomansky@googlemail.com>",
  "license": "MIT",
  "dependencies": {
    "@saeris/vue-spinners": "^1.0.8",
    "@types/sqlite3": "^3.1.6",
    "core-js": "^3.6.5",
    "electron": "^10.1.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-unhandled": "^3.0.2",
    "isomorphic-fetch": "^3.0.0",
    "mobx": "^6.0.0",
    "mobx-vue": "^2.0.10",
    "sqlite3": "^5.0.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.4.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/cli-plugin-babel": "~4.5.6",
    "@vue/cli-plugin-eslint": "~4.5.6",
    "@vue/cli-plugin-typescript": "~4.5.6",
    "@vue/cli-plugin-unit-jest": "~4.5.6",
    "@vue/cli-service": "~4.5.6",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "1.1.0",
    "cross-env": "^7.0.2",
    "electron-builder": "^22.8.1",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^10.0.2",
    "typescript": "~4.0.3",
    "vue-template-compiler": "^2.6.12"
  },
  "build": {
    "appId": "collection-tracker",
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "release"
    }
  }
}
